<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smallraw.coroutinedemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CoroutineScopeActivity"></activity>
        <activity android:name=".CoroutineContextActivity" />
        <activity android:name=".CancelCoroutinesActivity" />
        <activity android:name=".CiroutinersCooperationActivity" />
        <activity android:name=".ToggleCoroutinesActivity" />
        <activity android:name=".StartCoroutinesActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>